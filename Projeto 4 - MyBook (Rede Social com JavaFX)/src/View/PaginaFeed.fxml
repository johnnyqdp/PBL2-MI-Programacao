<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.PaginaFeedController">
   <children>
      <AnchorPane>
         <children>
            <ImageView fx:id="background" blendMode="MULTIPLY" depthTest="ENABLE" fitHeight="450.0" fitWidth="316.0">
               <image>
                  <Image url="@../arquivosdainterface/background.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <AnchorPane>
         <children>
            <ImageView fitHeight="63.0" fitWidth="316.0" pickOnBounds="true">
               <image>
                  <Image url="@../arquivosdainterface/hud1.jpg" />
               </image>
            </ImageView>
            <Button fx:id="botaoPerfil" layoutX="228.0" layoutY="19.0" mnemonicParsing="false" onAction="#apertaBotaoPerfil" opacity="0.0" prefHeight="25.0" prefWidth="27.0" />
            <Button fx:id="botaoBusca" layoutX="269.0" layoutY="19.0" mnemonicParsing="false" onAction="#apertaBotaoBusca" opacity="0.0" prefHeight="25.0" prefWidth="27.0" />
            <HBox fx:id="solicitacoes" alignment="CENTER" layoutY="80.0" prefHeight="63.0" prefWidth="316.0" />
            <HBox alignment="CENTER" layoutY="167.0" prefHeight="223.0" prefWidth="316.0">
               <children>
                  <ListView fx:id="feed" prefHeight="223.0" prefWidth="285.0" />
               </children></HBox>
            <Label layoutX="14.0" layoutY="63.0" text="Solicitações de amizade:" />
            <Label layoutX="14.0" layoutY="148.0" text="Feed de notícias:" />
            <Button layoutX="172.0" layoutY="145.0" mnemonicParsing="false" onAction="#apertaBotaoPostagem" prefHeight="23.0" prefWidth="129.0" text="+ Escrever uma Postagem">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button layoutX="180.0" layoutY="390.0" mnemonicParsing="false" onAction="#apertaBotaoVerMaisPosts" text="Ver mais antigos &gt;&gt;" />
            <Button layoutX="14.0" layoutY="390.0" mnemonicParsing="false" onAction="#apertaBotaoVoltarPosts" text="&lt;&lt; Ver mais recentes" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
